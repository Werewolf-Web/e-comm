{
  "name": "e-commerse",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
  "dev": "vite --mode development --host=0.0.0.0",
    "json:products": "json-server --watch data/product.json --port 3001 --host 0.0.0.0",
    "json:register": "json-server --watch data/register.json --port 3002 --host 0.0.0.0",
    "json:orders": "json-server --watch data/order.json --port 3003 --host 0.0.0.0",
    "json:all": "concurrently \"npm run json:products\" \"npm run json:register\" \"npm run json:orders\"",
    "dev:all": "concurrently \"npm run dev\" \"npm run json:all\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/lab": "^7.0.1-beta.20",
    "@mui/material": "^7.3.6",
    "@mui/styled-engine-sc": "^7.3.6",
    "bootstrap": "^5.3.8",
    "react": "^19.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.0",
    "react-image-magnify": "^2.7.4",
    "react-inner-image-zoom": "^4.0.1",
    "react-medium-image-zoom": "^5.4.0",
    "react-router-dom": "^7.10.1",
    "react-zoom-pan-pinch": "^3.7.0",
    "styled-components": "^6.1.19",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "^7.2.4"
  }
}
